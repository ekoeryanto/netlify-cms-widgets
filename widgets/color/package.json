{
  "name": "netlify-cms-widget-color",
  "version": "3.0.1",
  "description": "Color widget for Netlify CMS.",
  "main": "dist/cjs/color.min.js",
  "browser": "dist/umd/color.min.js",
  "module": "dist/es/color.min.js",
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
